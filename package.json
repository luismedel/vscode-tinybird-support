{
    "name": "vscode-tinybird-support",
    "displayName": "Tinybird support for Visual Studio Code",
    "description": "This extension provides syntax highlighting for Tinybird data projects.",
    "version": "0.4.2",
    "engines": {
        "vscode": "^1.67.0"
    },
    "repository": { "url": "https://github.com/tinybirdco/vscode-tinybird-support" },
    "icon": "images/icon.png",
    "publisher": "TinybirdCo",
    "author": {
        "name": "Tinybird",
        "email": "hi@tinybird.co",
        "url": "https://tinybird.co"
    },
    "contributors": [
        {
            "name": "Luis Medel",
            "email": "luis@luismedel.com"
        }
    ],
    "license": "MIT",
    "categories": [
        "Programming Languages",
        "Notebooks",
        "Data Science"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "languages": [{
            "id": "tinybird",
            "aliases": ["Tinybird project", "tinybird"],
            "extensions": [".datasource", ".pipe", ".endpoint", ".incl"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "./images/icons/tb-light.png",
                "dark": "./images/icons/tb-dark.png"
            }
        }],
        "grammars": [{
            "language": "tinybird",
            "scopeName": "source.tinybird",
            "path": "./syntaxes/tinybird.tmLanguage.json"
        }],
        "commands": [
            {
                "command": "tinybird.fmt",
                "title": "Format SQL in Tinybird project",
                "enablement": "true"
            },
            {
                "command": "tinybird.sql",
                "title": "Run SQL in Tinybird project",
                "enablement": "true"
            }
        ],
        "configuration": {
            "title": "Tinybird",
            "properties": {
              "tinybird.venv": {
                "type": "string",
                "default": ".e",
                "markdownDescription": "Work inside this Python virtual env. For example `venv` or `.e`. Emtpy for none."
              },
              "tinybird.venvActivate": {
                "type": "string",
                "default": "bin/activate",
                "markdownDescription": "Activation script name. Default is `bin/activate` but for some shells you might need another one (for example `bin/activate.fish` for the fish shell)."
              }
            }
        },
        "configurationDefaults": {
            "venv": ".e",
            "venvActivate": "bin/activate"
        }
    },
	"scripts": {
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"typescript": "^4.8.4"
	}
}
